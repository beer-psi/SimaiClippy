<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="disqord" value="https://www.myget.org/F/disqord/api/v3/index.json" />
  </packageSources>

  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>

    <packageSource key="disqord">
      <package pattern="Disqord" />
      <package pattern="Disqord.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>